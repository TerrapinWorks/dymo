{
	"manifest_version": 2, 

	"name": "Terrapin Works Label Printing", 
	"description": "One click printing for the Dymo",
	"version": "0.1",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js":["contentscript.js"]
		}
	],
	
	"permissions": [
		"activeTab", "tabs", "http://*/*", "https://*/*"
	]
}

